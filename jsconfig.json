{
  "compilerOptions": {
    "target": "es6",
    "jsx": "preserve", 
    "baseUrl": "src",
    "paths": {
      "apis/*": ["./src/apis/*"],
      "constants/*": ["./src/constants/*"],
      "utils/*": ["./src/utils/*"],
      "components/*": ["./src/components/*"],
      "containers/*": ["./src/containers/*"],
      "Socket/*": ["./src/socket/*"],
      "sections/*": ["./src/sections/*"]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
